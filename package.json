{
  "name": "obie",
  "version": "0.0.1",
  "scripts": {
    "build:server": "parcel build ./src/server/index.ts --target node",
    "build": "run-p -l build:server",
    "lint": "eslint \"*/**/*.{ts,tsx}\" --fix",
    "start": "cross-env NODE_ENV=dev run-p -rl tsc tsnd",
    "tsnd": "tsnd --transpile-only ./src/server/index.ts",
    "tsc": "tsc --pretty --noEmit --watch",
    "test": "jest --forceExit --detectOpenHandles"
  },
  "husky": {
    "hooks": {
      "pre-commit": "run-p -l lint test"
    }
  },
  "dependencies": {
    "apollo-server-express": "^2.10.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "graphql": "^14.6.0",
    "parcel-bundler": "1.12.4",
    "sequelize": "^6.28.2",
    "sqlite3": "^4.1.1",
    "typescript": "^3.7.5"
  },
  "devDependencies": {
    "@types/express": "^4.17.2",
    "@types/jest": "^25.1.2",
    "@types/node": "^13.7.2",
    "@types/parcel-bundler": "^1.12.1",
    "@types/ramda": "^0.26.41",
    "@typescript-eslint/eslint-plugin": "^2.20.0",
    "@typescript-eslint/parser": "^2.20.0",
    "cross-env": "^7.0.1",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.18.3",
    "husky": "^4.2.3",
    "jest": "^25.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.19.1",
    "ts-jest": "^25.2.0",
    "ts-node": "^8.6.2",
    "ts-node-dev": "^1.0.0-pre.44",
    "umzug": "3.0.0-beta.3"
  }
}
